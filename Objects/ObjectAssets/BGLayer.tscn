script/source = "extends CanvasLayer


@onready var polygons_nodes = get_children()

func _physics_process(delta):
	
	for i in range(polygons_nodes.size()):
		var node = polygons_nodes[i]
		var x:int
		
		match i%4:
			0: x = 8
			1: x = 2
			2: x = -2
			3: x = -8
		
		
		node.rotation += (delta * x *0.02)
		
	
	
"

